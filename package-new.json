{
  "name": "reloconnect-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "RELOConnect - Comprehensive Relocation Platform Monorepo",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "services/*",
    "libs/*",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm run --parallel dev",
    "build": "pnpm run --recursive build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "pnpm run --recursive lint",
    "type-check": "tsc --noEmit",
    "clean": "pnpm run --recursive clean && rm -rf node_modules",
    "fresh": "pnpm run clean && pnpm install",
    "prepare": "husky install",
    "start": "expo start",
    "start:clear": "expo start --clear",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "user-app": "cd apps/user-app && npm start",
    "driver-app": "cd apps/driver-app && npm start"
  },
  "dependencies": {
    "expo": "~53.0.17",
    "expo-router": "~4.0.15",
    "expo-status-bar": "~2.2.3",
    "expo-location": "~18.0.4",
    "expo-maps": "~0.15.0",
    "expo-font": "~13.0.4",
    "expo-splash-screen": "~0.29.13",
    "expo-constants": "~17.0.3",
    "expo-linking": "~7.0.3",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.2.4",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/bottom-tabs": "^7.0.15",
    "@react-navigation/stack": "^7.0.15",
    "@react-navigation/drawer": "^7.0.15",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-vector-icons": "^10.2.0",
    "@expo/vector-icons": "^14.1.0",
    "nativewind": "^2.0.11",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "@testing-library/react-native": "^12.10.1",
    "@testing-library/jest-native": "^5.4.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.3.0",
    "tailwindcss": "^3.4.0"
  },
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "resolutions": {
    "expo-modules-core": "~2.1.3",
    "undici": "6.21.3",
    "@types/react": "18.3.12"
  }
}
